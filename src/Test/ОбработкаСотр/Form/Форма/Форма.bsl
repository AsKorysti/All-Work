
&НаСервере
Процедура ОКНаСервере()
	ВыборкаДокументов = Документы.ОказаниеУслуги.Выбрать(Дата(2000,01,01,01,01,01), КонецГода(ТекущаяДата())); 
	
	Пока  ВыборкаДокументов.Следующий() Цикл 

		
		Если ВыборкаДокументов.Мастер = Сотрудник Тогда 
			Сообщить(ВыборкаДокументов.Номер);	
		КонецЕсли;
	КонецЦикла;
	
КонецПроцедуры

&НаКлиенте
Процедура ОК(Команда)
	ОКНаСервере();
КонецПроцедуры
